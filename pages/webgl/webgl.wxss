.div1 {
    position: relative;
    height: 200px;
    width: 200px;
    margin: 50px;
    padding: 10px;
    border: 1px solid black;
    perspective: 200;
    -webkit-perspective: 200;
    transform: translateX(0px) translateY(0px) translateZ(0px);
    -webkit-transform: translateX(0px) translateY(0px) translateZ(0px);
    /* Safari and Chrome */
}

.div2 {
    padding: 50px;
    position: absolute;
    border: 1px solid black;
    background-color: red;
    transform: rotateX(45deg) translateX(0px) translateY(0px) translateZ(0px);
    -webkit-transform: rotateX(45deg) rotateY(45deg) translateX(0px) translateY(0px) translateZ(0px);
    /* Safari and Chrome */
}

page {
    padding: 0;
    margin: 0;
}

page {
    position: relative;
    height: 100%;
    background-color: #F6F6F6
}

.imgpika{
    perspective: 800px;  
}

.rotateCtn {
    position: absolute;
    width: 70%;
    height: 70%;
    left: 15%;
    bottom: 20%;
    transform-style: preserve-3d;
    perspective: 800px;
}

.frame {
    position: absolute;
    height: 100%;
    width: 100%;
}

.frame image {
    height: 100%;
    width: 100%;
    border-radius: 8px;
}

.front {
    animation: front 1s linear 1;
    backface-visibility: hidden;
}

.back {
    animation: back 1s linear 1;
}

@keyframes front {
    from {
        transform: rotateY(0deg);
    }

    to {
        transform: rotateY(180deg);
    }
}

@keyframes back {
    from {
        transform: rotateY(-180deg);
    }

    to {
        transform: rotateY(0deg);
    }
}

:root,
body {
    height: 100%;
    perspective: 1000px;
}

@keyframes run {
    0% {
        transform: rotateY(0deg);
    }

    100% {
        transform: rotateY(360deg);
    }
}

.wra {
    position: absolute;
    width: 200px;
    height: 100px;
    left: calc(50% - 100px);
    top: calc(50% - 50px);
    transform-style: preserve-3d;
    animation: run 20s linear infinite;
}

.img {
