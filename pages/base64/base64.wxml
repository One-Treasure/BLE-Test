<view class="section">
  <view class="btn-area">
    <button size="mini" bindtap="uploadImage">Upload Image</button>
    <button size="mini" bindtap="processImage">Process Image</button>
  </view>
  <image class="img" bindload="imageLoad" style="width: {{imagewidth}}px; height: {{imageheight}}px;" src="{{imgUrl}}"></image>
  <image src="{{base64}}" style="width: 100%" bind:tap="previewImage"></image>
  <canvas style="width: 300px; height: 200px;visibility:hidden;" canvas-id="myCanvas"></canvas>
  <canvas style="width:2px;height:2px;visibility:hidden;" canvas-id="judgeCanvas"></canvas>
</view>