/* 圆形进度条 */

.circleBar {
  width: 100%;
  height: auto;
  overflow: hidden;
  position: relative;
}

.cir {
  display: inline-block;
  background-color: #fff;
  border-radius: 100%;
}

.top {
  text-align: center;
}

.centerWord {
  width: 100%;
  position: absolute;
  top: 40px;
  left: 0;
  text-align: center;
  color: #3686ff;
}

.navigator-hover {
  /* 设置透明度 */
  opacity: 0.5;
}